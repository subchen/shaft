<?xml version="1.0" encoding="utf-8" ?>

<table display-name="文件" description="数据库表总揽">

    <column name="name"
            type="varchar(50)"
            display-name="名称"
            />
            
    <column name="type"
            type="varchar(50)"
            display-name="类型"
            />
            
